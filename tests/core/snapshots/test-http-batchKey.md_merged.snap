---
source: tests/core/spec.rs
expression: formatter
---
schema @server(port: 8000) @upstream(baseURL: "http://jsonplaceholder.typicode.com") {
  query: Query
}

type Bar {
  barName: String!
  id: ID!
}

type Foo {
  barId: String!
  bars: [Bar!]!
    @http(
      batchKey: ["bars", "id"]
      path: "/bar"
      query: [{key: "baz", value: "static_value"}, {key: "barId[]", value: "{{.value.barId}}"}]
      batch: {delay: 10, headers: [], maxSize: 1000}
    )
  fooName: String!
  id: ID!
}

type FooResponse {
  foos: [Foo!]!
}

type Query {
  foos: FooResponse @http(path: "/foo")
}
