{
  "input": [
    {
      "import": {
        "src": "../protobuf/news.proto"
      }
    },
    {
      "config": {
        "src": "../configs/user-posts.graphql"
      }
    },
    {
      "import": {
        "src": "https://jsonplaceholder.typicode.com/posts/1"
      }
    }
  ],
  "generate": {
    "schema": {
      "query": "Query"
    }
  },
  "output": {
    "useLinks": true,
    "format": "graphQL",
    "file": "./output.graphql"
  },
  "transform": {
    "ambiguousNameResolver": {
      "inputPrefix": "IN",
      "outputPrefix": "OUT",
      "inputPostFix": "IN",
      "outputPostFix": "OUT"
    },
    "mergeTypes": {
      "threshold": 0.9
    },
    "js": {
      "source": "transformer.js",
      "functionName": "transform"
    }
  }
}
