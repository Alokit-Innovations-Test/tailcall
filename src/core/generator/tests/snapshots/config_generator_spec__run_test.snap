---
source: src/core/generator/tests/config_generator_spec.rs
expression: config_writer.config.to_sdl()
---
schema @server(hostname: "0.0.0.0", port: 8000) @upstream(baseURL: "https://jsonplaceholder.typicode.com", httpCache: 42) {
  query: Query
}

type Address {
  city: String
  geo: Geo
  street: String
  suite: String
  zipcode: String
}

type Company {
  bs: String
  catchPhrase: String
  name: String
}

type F1 {
  body: String
  email: String
  id: Int
  name: String
  postId: Int
}

type F2 {
  body: String
  id: Int
  title: String
  userId: Int
}

type F3 {
  address: Address
  company: Company
  email: String
  id: Int
  name: String
  phone: String
  username: String
  website: String
}

type Geo {
  lat: String
  lng: String
}

type Post {
  body: String!
  id: Int!
  title: String!
  user: User @http(path: "/users/{{.value.userId}}")
  userId: Int!
}

type Query {
  f1: [F1] @http(path: "/comments")
  f2(p1: Int!): F2 @http(path: "/posts/{{.args.p1}}")
  f3(p1: Int!): F3 @http(path: "/users/{{.args.p1}}")
  post(id: Int!): Post @http(path: "/posts/{{.args.id}}")
  posts: [Post] @http(path: "/posts")
  user(id: Int!): User @http(path: "/users/{{.args.id}}")
  users: [User] @http(path: "/users")
}

type User {
  email: String!
  id: Int!
  name: String!
  phone: String
  username: String!
  website: String @expr(body: "/users/website/{{.value.username}}")
}
